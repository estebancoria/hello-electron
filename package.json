{
  "name": "electron-workshop",
  "version": "0.0.1",
  "description": "Learning using Electron lib",
  "main": "main.js",
  "productName": "Electron App",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package:osx": "electron-packager . --platform=darwin --arch=x64",
